<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/main.css">
</head>
<body>
    <div class="site-main">
        <div class="site-nav">
        <%- include('../includes/nav') %>
        </div>
        <div class="main-content">
            <table>
                <tr>
                    <th>Title</th>
                    <th>URL</th>
                    <th>Actions</th>
                </tr>
                    <% pages.forEach(page => { %>
                <tr>

                        <td><%- page.title %></td>
                        <td><%- page.url %></td>
                        <td>
                            <a href="/admin/pages/<%- page.ID %>/edit/">Edit</a> |
                            <a href="/admin/pages/<%- page.ID %>/delete/">Delete</a>
                        </td>
                </tr>

                       <% }) %>
                

        </table>

        </div>
    </div>
</body>
</html>